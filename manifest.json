{
  "name": "TU Dresden Auto Login",
  "version": "3.4.2.0",
  "description": "Spare dir Klicks und nutze TU Dresden Auto Login!",
  "permissions": [
    "storage", "system.cpu"
  ],
  "optional_permissions": [],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "js": [
        "contentScripts/content_idp2_1.js"
      ],
      "run_at": "document_start",
      "matches": [
        "*://idp2.tu-dresden.de/idp/profile/*"
      ]
    },
    {
      "js": [
        "contentScripts/content_fwd_jexam.js"
      ],
      "run_at": "document_start",
      "matches": [
        "*://jexam.inf.tu-dresden.de/",
        "*://www.google.de/search?q=jexam*",
        "*://www.google.com/search?q=jexam*",
        "*://www.google.de/search*&q=jexam&oq=jexam*",
        "*://www.google.com/search*&q=jexam&oq=jexam*"
      ]
    },
    {
      "js": [
        "contentScripts/content_fwd_cloudstore.js"
      ],
      "run_at": "document_start",
      "matches": [
        "*://www.google.de/search?q=tustore*",
        "*://www.google.com/search?q=tustore*",
        "*://www.google.de/search*&q=tustore&oq=tustore*",
        "*://www.google.com/search*&q=tustore&oq=tustore*"
      ]
    },
    {
      "js": [
        "contentScripts/content_fwd_owa.js"
      ],
      "run_at": "document_start",
      "matches": [
        "*://www.google.de/search?q=tumail*",
        "*://www.google.de/search?q=tudmail*",
        "*://www.google.com/search?q=tumail*",
        "*://www.google.com/search?q=tudmail*",
        "*://www.google.de/search*&q=tumail&oq=tumail*",
        "*://www.google.com/search*&q=tumail&oq=tumail*",
        "*://www.google.de/search*&q=tudmail&oq=tudmail*",
        "*://www.google.com/search*&q=tudmail&oq=tudmail*"
      ]
    },
    {
      "js": [
        "contentScripts/content_jexam.js"
      ],
      "run_at": "document_start",
      "matches": [
        "*://jexam.inf.tu-dresden.de/*"
      ]
    },
    {
      "js": [
        "thirdParty/jquery-3.4.1.min.js", 
        "thirdParty/Chart.bundle.min.js",
        "contentScripts/pimpHISQIS.js"
      ],
      "run_at": "document_start",
      "matches": [
        "https://qis.dez.tu-dresden.de/qisserver/servlet/*"
      ],
      "include_globs":[
        "*state=notenspiegelStudent*&next=list.vm&nextdir=qispos/notenspiegel/student&createInfos=Y&struct=auswahlBaum&nodeID=auswahlBaum*expand=0*auswahlBaum*"
      ]
    },
    {
      "js": [
        "contentScripts/content_fwd_opal.js"
      ],
      "run_at": "document_start",
      "matches": [
        "*://opal.de/*",
        "*://www.google.de/search?q=opal*",
        "*://www.google.com/search?q=opal*",
        "*://www.google.de/search*&q=opal&oq=opal*",
        "*://www.google.com/search*&q=opal&oq=opal*"
      ]
    },
    {
      "js": [
        "contentScripts/content_selma.js"
      ],
      "run_at": "document_start",
      "matches": [
        "*://selma.tu-dresden.de/*"
      ]
    },
    {
      "js": [
        "contentScripts/content_fwd_hisqis.js"
      ],
      "run_at": "document_start",
      "matches": [
        "*://www.google.de/search?q=hisqis*",
        "*://www.google.com/search?q=hisqis*",
        "*://www.google.de/search*&q=hisqis&oq=hisqis*",
        "*://www.google.com/search*&q=hisqis&oq=hisqis*"
      ]
    },
    {
      "js": [
        "contentScripts/content_fwd_selma.js"
      ],
      "run_at": "document_start",
      "matches": [
        "*://www.google.de/search?q=selma*",
        "*://www.google.com/search?q=selma*",
        "*://www.google.de/search*&q=selma&oq=selma*",
        "*://www.google.com/search*&q=selma&oq=selma*"
      ]
    },
    {
      "js": [
        "contentScripts/content_qis.js"
      ],
      "run_at": "document_start",
      "matches": [
        "*://qis.dez.tu-dresden.de/qisserver/servlet*"
      ]
    },
    {
      "js": [
        "contentScripts/content_bildungsportal.js"
      ],
      "run_at": "document_start",
      "matches": [
        "*://bildungsportal.sachsen.de/opal/resource?*",
        "*://bildungsportal.sachsen.de/opal/home*",
        "*://bildungsportal.sachsen.de/opal/auth/*"
      ]
    },
    {
      "js": [
        "contentScripts/content_fwd_matrix.js"
      ],
      "run_at": "document_start",
      "matches": [
        "*://www.google.com/search?q=tumatrix*",
        "*://www.google.de/search?q=tumatrix*",
        "*://www.google.de/search*&q=tumatrix&oq=tumatrix*",
        "*://www.google.com/search*&q=tumatrix&oq=tumatrix*"
      ]
    },
    {
      "js": [
        "contentScripts/content_matrix.js"
      ],
      "run_at": "document_end",
      "matches": [
        "*://matrix.tu-dresden.de/*"
      ],
      "include_globs": [
        "*/welcome",
        "*/login",
        "*://matrix.tu-dresden.de/"
      ]
    },
    {
      "js": [
        "contentScripts/content_owa.js"
      ],
      "run_at": "document_start",
      "matches": [
        "*://msx.tu-dresden.de/*"
      ]
    },
    {
      "js": [
        "contentScripts/content_cloudstore.js"
      ],
      "run_at": "document_start",
      "matches": [
        "*://cloudstore.zih.tu-dresden.de/*"
      ]
    },
    {
      "js": [
        "contentScripts/content_bildungsportal_main.js"
      ],
      "run_at": "document_start",
      "matches": [
        "*://bildungsportal.sachsen.de/opal/shiblogin*",
        "*://bildungsportal.sachsen.de/opal/login*"
      ]
    },
    {
      "js": [
        "contentScripts/content_bildungsportal_logout.js"
      ],
      "run_at": "document_start",
      "matches": [
        "*://bildungsportal.sachsen.de/opal*"
      ]
    },
    {
      "js": ["contentScripts/content_magma.js"],
      "run_at": "document_start",
      "matches": [
        "*://bildungsportal.sachsen.de/magma*"
      ]
    },
    {
      "js": ["contentScripts/content_fwd_magma.js"],
      "run_at": "document_start",
      "matches": [
        "https://www.google.com/search?q=magma*",
        "https://www.google.de/search?q=magma*",
        "*://www.google.de/search*&q=magma&oq=magma*",
        "*://www.google.com/search*&q=magma&oq=magma*"
        
      ]
    }
  ],
  "icons": {
    "16": "images/autologin16.png",
    "32": "images/autologin32.png",
    "48": "images/autologin48.png",
    "128": "images/autologin128.png"
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "options_page": "register_user.html",
  "manifest_version": 2
}